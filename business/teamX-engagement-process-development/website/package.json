{
  "name": "website",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t teamx-website .",
    "docker:build-dev": "docker build -f Dockerfile.dev -t teamx-website-dev .",
    "docker:run": "docker run -p 3000:3000 --name teamx-website teamx-website",
    "docker:run-dev": "docker run -p 3001:3000 -v $(pwd):/app -v /app/node_modules --name teamx-website-dev teamx-website-dev",
    "docker:stop": "docker stop teamx-website teamx-website-dev || true",
    "docker:clean": "docker rm teamx-website teamx-website-dev || true",
    "docker:logs": "docker logs teamx-website",
    "compose:up": "docker-compose up -d",
    "compose:up-dev": "docker-compose --profile dev up -d",
    "compose:down": "docker-compose down",
    "compose:logs": "docker-compose logs -f",
    "compose:build": "docker-compose build",
    "deploy:local": "npm run build && npm run docker:build && npm run compose:up",
    "deploy:dev": "npm run docker:build-dev && npm run compose:up-dev",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "playwright:mcp": "npx @playwright/mcp@latest",
    "playwright:mcp-headless": "npx @playwright/mcp@latest --browser chromium --headless --viewport-size \"1920,1080\"",
    "playwright:mcp-firefox": "npx @playwright/mcp@latest --browser firefox --viewport-size \"1920,1080\"",
    "playwright:mcp-webkit": "npx @playwright/mcp@latest --browser webkit --viewport-size \"1920,1080\"",
    "playwright:test": "node automation/run-tests.js",
    "playwright:screenshots": "node automation/take-screenshots.js",
    "playwright:setup": "mkdir -p screenshots && mkdir -p automation",
    "playwright:validate": "node automation/test-mcp-setup.js"
  },
  "keywords": [
    "healthcare",
    "consulting",
    "teamx",
    "next.js",
    "typescript"
  ],
  "author": "TeamX",
  "license": "ISC",
  "description": "TeamX Healthcare Consulting - Professional website showcasing healthcare consulting services",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@playwright/mcp": "^0.0.35",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "next": "^16.0.1",
    "playwright": "^1.55.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2"
  }
}
